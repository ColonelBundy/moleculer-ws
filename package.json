{
  "name": "moleculer-ws",
  "version": "1.0.0",
  "description": "Websocket gateway for moleculer",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "compile": "yarn clean && tsc && node src/dist/index.js",
    "dev": "nodemon -e ts --exec \"yarn compile\"",
    "testing": "yarn clean && tsc && node dist/src/testing/service.js",
    "testing2": "yarn clean && tsc && node dist/src/testing/second.service.js",
    "debug": "yarn clean && tsc && node --inspect dist/src/index.js"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "author": "Colonelbundy <colonelbundy@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "es6-error": "^4.1.1",
    "eventemitter2": "^5.0.1",
    "ioredis": "^3.2.2",
    "lodash": "^4.17.4",
    "moleculer": "^0.11.10",
    "moleculer-decorators": "^1.0.11",
    "nanomatch": "^1.2.7",
    "nanotimer": "^0.3.15",
    "shortid": "^2.2.8",
    "uws": "^9.14.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/bluebird": "^3.5.20",
    "@types/http-status": "^0.2.30",
    "@types/ioredis": "^3.2.5",
    "@types/lodash": "^4.14.100",
    "@types/node": "^9.3.0",
    "@types/uws": "^0.13.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "typescript": "^2.7.1"
  }
}
